{
    "name": "flarum/composer-installer",
    "type": "composer-installer",
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "composer/composer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Flarum\\Composer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flarum\\Composer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "class": "Flarum\\Composer\\Installer"
    }
}
